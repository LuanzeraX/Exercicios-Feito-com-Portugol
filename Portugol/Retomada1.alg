INICIO
  VARIAVEL INTEIRO A
  VARIAVEL INTEIRO B
  VARIAVEL INTEIRO SOMA
  VARIAVEL INTEIRO SUBTRAI
  
  ESCREVER "ENTRE COM O NÚMERO DE A:"
  LER A
  ESCREVER "ENTRE COM O NÚMERO DE B:"
  LER B
  
  SE A>B ENTÃO
      SOMA <- A + B
      ESCREVER "SOMA DE A + B = ", SOMA
  SENÃO
      SUBTRAI <- A - B
      ESCREVER "SUBTRACAO DE A - B = ", SUBTRAI
  FIMSE
FIM      