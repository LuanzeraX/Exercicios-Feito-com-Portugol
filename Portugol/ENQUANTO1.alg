INICIO
    VARIAVEL INTEIRO TAB, CONTADOR, RESULTADO 
    CONTADOR <- 1
    RESULTADO <- 0
    ESCREVER "DIGITE A TABUADA DESEJADA : "
    LER TAB 
    ENQUANTO CONTADOR < 11 FAZ
        RESULTADO <- TAB * CONTADOR
        ESCREVER TAB, " X ", CONTADOR, " = ", RESULTADO, "\n"
        CONTADOR <- CONTADOR + 1
        FIMENQUANTO
        FIM