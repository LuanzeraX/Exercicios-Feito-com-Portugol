INICIO
    VARIAVEL INTEIRO CONTADOR
    VARIAVEL INTEIRO NUMERO[5]
    CONTADOR <- 0
    PARA CONTADOR DE 0 ATE 4 PASSO 1
        ESCREVER " DIGITE O ", CONTADOR+1, "º. NÚMERO:"
        LER NUMERO[CONTADOR]
        PROXIMO
        
        ESCREVER "OS NÚMEROS DIGITADOS FORAM:" , "\n"
        
        PARA CONTADOR DE 0 ATE 4 PASSO 1
        ESCREVER NUMERO[CONTADOR], "\n"
        PROXIMO
        FIM
         