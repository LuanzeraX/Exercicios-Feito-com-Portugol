INICIO
    VARIAVEL INTEIRO CONTADOR
    VARIAVEL TEXTO NOME[5]
    CONTADOR <- 0
    PARA CONTADOR DE 0 ATE 4 PASSO 1
        ESCREVER " DIGITE O ", CONTADOR+1, "º. NOME:"
        LER NOME [ CONTADOR ]
        PROXIMO
        
        ESCREVER "OS NOME DIGITADOS FORAM:" , "\n"
        
        PARA CONTADOR DE 0 ATE 4 PASSO 1
        ESCREVER NOME[CONTADOR], "\n"
        PROXIMO
        FIM


