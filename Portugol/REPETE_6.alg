INICIO
    VARIAVEL TEXTO NUMERO
    VARIAVEL TEXTO GUARDA_NUMERO
    VARIAVEL TEXTO CONTINUA
    
    REPETE
        ESCREVER "DIGITE UM NÚMERO: "
        LER NUMERO
        GUARDA_NUMERO <- NUMERO
        ESCREVER "DESEJA CONTINUAR? (S/N)"
        LER CONTINUA
        ESCREVER "OS NÚMEROS DIGITADOS SÃO: ", GUARDA_NUMERO 
     ATE CONTINUA = "S" 
  FIM