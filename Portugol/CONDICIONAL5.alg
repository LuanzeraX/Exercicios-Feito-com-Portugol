INICIO
    VARIAVEL INTEIRO NUMERO 
    VARIAVEL INTEIRO RESTO
    
    ESCREVER "DIGITE UM NUMERO "
    LER NUMERO
    
    RESTO <- NUMERO%3
    
    SE RESTO = 0 ENTÃO 
    ESCREVER " NÚMERO ",NUMERO," É MULTIPLO DE TRÊS "
    SENÃO 
    ESCREVER " NÚMERO ",NUMERO," NÃO É MULTIPLO DE TRÊS "
    FIMSE 
    FIM
   